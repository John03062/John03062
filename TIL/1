#include <stdio.h>

// 최대공약수(GCD) 계산 함수
int gcd(int a, int b) {
    while (b != 0) {
        int temp = a % b;
        a = b;
        b = temp;
    }
    return a;
}

// 최소공배수(LCM) 계산 함수
int lcm(int a, int b) {
    return a * b / gcd(a, b); // LCM(a, b) = (a * b) / GCD(a, b)
}

int main() {
    int a, b;
    
    // 두 수 입력받기
    scanf("%d %d", &a, &b);
    
    // 최대공약수와 최소공배수 출력
    printf("%d\n", gcd(a, b));
    printf("%d\n", lcm(a, b));
    
    return 0;
}
